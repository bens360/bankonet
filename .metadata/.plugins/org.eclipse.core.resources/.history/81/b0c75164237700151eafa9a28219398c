package command;

public class ExitCommand implements IhmCommand{
		
	private String id;
	private String libelleMenu;
	
	@Override
	public String getId() {
		return id;
	}

	@Override
	public String getLibelleMenu() {
		// TODO Auto-generated method stub
		return libelleMenu;
	}

	@Override
	public void setId() {
		id = "0";
		
	}

	@Override
	public void setLibelleMenu() {
		libelleMenu = "Arrêter le programme";
	}
	
}


